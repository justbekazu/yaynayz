<div class="container-fluid mt-3">
    <div class="row">
      <div class="col">
        <h3><strong>Chuck can't find your shortcode</strong></h3>
      </div>
      <div class="col text-end">
        <a href="/" class="btn btn-warning">Try Again</a>
      </div>
    </div>
    {{#if questions.length}}
    <div class="row">
      {{#each questions as |question|}}
      <div class="col-4 mt-3 mb-2">
        {{> question-info-edit question }}
      </div>
      {{/each}}
    </div>
  {{else}}
  <div class="row mt-3">
    <div class="col text-center">
      <div class="text-center">
        <img src="https://media.giphy.com/media/AhwuGl0MPNzyg/giphy.gif" class="rounded" width="300" alt="Do it now!">
      </div>
    </div>
  </div>
  {{/if}}
</div>